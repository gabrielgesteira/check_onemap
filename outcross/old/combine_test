# Testing combinations
combinations = combn(types,2)
# Setting sequence of marker types
guide = matrix(seq(1,18,1),18,1)
types = matrix(NA, 18,1)
for(i in 1:nrow(types)){
  if  (guide[i] == 1) {
    types[i,] = paste0("A.1")}
  else if (guide[i] == 2) {
    types[i,] = paste0("A.2")}
  else if (guide[i] == 3) {
    types[i,] = paste0("A.3")}
  else if (guide[i] == 4) {
    types[i,] = paste0("A.4")}
  else if (guide[i] == 5) {
    types[i,] = paste0("B1.5")}
  else if (guide[i] == 6) {
    types[i,] = paste0("B2.6")}
  else if (guide[i] == 7) {
    types[i,] = paste0("B3.7")}
  else if (guide[i] == 8) {
    types[i,] = paste0("C.8")}
  else if (guide[i] == 9) {
    types[i,] = paste0("D1.9")}
  else if (guide[i] == 10) {
    types[i,] = paste0("D1.10")}
  else if (guide[i] == 11) {
    types[i,] = paste0("D1.11")}
  else if (guide[i] == 12) {
    types[i,] = paste0("D1.12")}
  else if (guide[i] == 13) {
    types[i,] = paste0("D1.13")}
  else if (guide[i] == 14) {
    types[i,] = paste0("D2.14")}
  else if (guide[i] == 15) {
    types[i,] = paste0("D2.15")}
  else if (guide[i] == 16) {
    types[i,] = paste0("D2.16")}
  else if (guide[i] == 17) {
    types[i,] = paste0("D2.17")}
  else if (guide[i] == 18) {
    types[i,] = paste0("D2.18")}
}

